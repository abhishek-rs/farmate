{
  "name": "farmate",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://abhishek-rs.github.io/farmate/",
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7",
    "rerun-script": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "dark-sky-api": "^0.5.28",
    "firebase": "^4.1.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "node-geocoder": "^3.18.0",
    "primereact": "^1.0.0-alpha.2",
    "react": "^15.6.1",
    "react-animated-weather": "^0.1.6",
    "react-bootstrap": "^0.31.0",
    "react-d3-basic": "^1.6.11",
    "react-d3-brush": "^1.1.0",
    "react-d3-core": "^1.3.9",
    "react-d3-tooltip": "^1.4.14",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tooltip": "^3.3.0",
    "reactfire": "^1.0.0",
    "superagent": "^3.5.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "watch": "rerun-script",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "watches": {
    "start": [
      "./src/*.css",
      "./src/**/*.css"
    ]
  }
}
